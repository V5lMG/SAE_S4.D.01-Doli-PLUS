{
  "name": "yasmf/yasmf_hello_world",
  "config": {
    "vendor-dir": "hello_world/lib/vendor"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/the-yasmf-project/yasmf"
    }
  ],
  "autoload": {
    "psr-4": {
      "controllers\\": "doli_plus/controllers",
      "services\\": "doli_plus/services",
      "application\\": "doli_plus/application"
    }
  },
  "require": {
    "php": "8.*",
    "phpunit/phpunit": "10.0.19",
    "phpstan/phpstan": "1.10.67",
    "ext-pdo": "*",
    "yasmf/yasmf": ">=1.6.2"
  }
}